/********* common *********/
/* 小模块背景色: #F2F5FC */
/* 渐变色蓝: rgb(231, 237, 252) */
/* 渐变色粉: #FF8691 */

/* 背景色 */
.common_bg_color {
  background-color: rgb(231, 237, 252);
}

/* 阴影 */
.common_shadow {
  box-shadow: 0 0 30rpx 5rpx #92a5fe50;
}

.common_shadow2 {
  box-shadow: 0 0 20rpx 5rpx #9e9e9e30;
}

/* 文字内容 */
.common_title {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 35rpx;
  font-weight: bolder;
  color: #324faca4;
  margin-bottom: 5rpx;
}
.common_subtitle {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 25rpx;
  font-weight: bold;
  color: rgb(179, 179, 179);
  padding: 5rpx 0;
  margin-bottom: 10rpx;
}
.common_content_box {
  box-sizing: border-box;
  width: 650rpx;
  padding: 40rpx;
  border-radius: 30rpx;
  background-color: #F2F5FC05;
}
.common_content {
  line-height: 45rpx;
  font-size: 28rpx;
  font-weight: light;
  color: rgb(71, 71, 71);
}
.text_overflow_two{
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.text_overflow_one{
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.text_of_lines1 {
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.text_of_lines2 {
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.text_of_lines3 {
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

/* Loading */
.loading_view {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* 布局 */
.flex_column_box {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.flex_column_center {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.flex_column_start {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.flex_column_space {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}
.flex_row_center {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.flex_row_start_center {
  display: flex;
  flex-direction: row;
  justify-content: start;
  align-items: center;
}
.flex_row_start {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
}
.flex_row_space {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.flex_row_end {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.flex_row_start_end {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-end;
}

/********* iconFont *********/
@font-face {
  font-family: 'iconfont';
  src: url('iconfont.eot');
  font-weight: 500;
  font-style: normal;
}
@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff;charset=utf-8;base64,') format('woff'),
      url('iconfont.ttf') format('truetype'),
      url('iconfont.svg#iconfont') format('svg');
  font-weight: 500;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

